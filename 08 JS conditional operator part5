//var 2
const input1 = prompt('Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ Ð´Ð²Ð° Ð½Ð°Ñ‚ÑƒÑ€Ð°Ð»ÑŒÐ½Ñ‹Ñ… Ñ‡Ð¸ÑÐ»Ð° Ñ‡ÐµÑ€ÐµÐ· Ð¿Ñ€Ð¾Ð±ÐµÐ» (m Ð¸ n)').split(' ').map(Number);
const input2 = prompt('Ð’Ð²ÐµÐ´Ð¸Ñ‚Ðµ Ð´Ð²Ð° Ð½Ð°Ñ‚ÑƒÑ€Ð°Ð»ÑŒÐ½Ñ‹Ñ… Ñ‡Ð¸ÑÐ»Ð° Ñ‡ÐµÑ€ÐµÐ· Ð¿Ñ€Ð¾Ð±ÐµÐ» (x Ð¸ y)').split(' ').map(Number);

const m = input1[0];
const n = input1[1];
const x = input2[0];
const y = input2[1];

function isValidCell(i, j) {
    return i >= 1 && i <= m && j >= 1 && j <= n;
}

function printNeighbours(i, j) {
    const neighbours = [
        [i-1, j], // Ð»ÐµÐ²Ð°Ñ ÐºÐ»ÐµÑ‚ÐºÐ°
        [i, j-1], // Ð²ÐµÑ€Ñ…Ð½ÑÑ ÐºÐ»ÐµÑ‚ÐºÐ°
        [i+1, j], // Ð¿Ñ€Ð°Ð²Ð°Ñ ÐºÐ»ÐµÑ‚ÐºÐ°
        [i, j+1]  // Ð½Ð¸Ð¶Ð½ÑÑ ÐºÐ»ÐµÑ‚ÐºÐ°
    ];

    for (let neighbour of neighbours) {
        if (isValidCell(neighbour[0], neighbour[1])) {
            console.log(neighbour[0] + ' ' + neighbour[1]);
        }
    }
}

printNeighbours(x, y);
