//var7, 8
let a = prompt().padStart(4, '0')
console.log(Number(a === a.split('').reverse().join(''))) // split - разбивает a на массив, reverse разворачивает его, join убирает массив, соединяя в строку
console.log(1 * (a === a.split('').reverse().join('')))


//var9
let num = prompt().padStart(4, '0')
let[a, b, c, d] = num
res = a - d + b - c + 1
