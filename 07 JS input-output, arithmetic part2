c = Array.from(String(a))
x = c[0] == c[3] & c[1] == c[2] ? 1 : 44
console.log(x)


//var4
let n = Number(prompt())
let d1 = Math.floor(n / 1000) % 10
let d2 = Math.floor(n / 100) % 10
let d3 = Math.floor(n / 10) % 10
let d4 = Math.floor(n / 1) % 10
console.log(1 + Math.abs(d1 - d4) + Math.abs(d2 - d3)) //abs - возводит в степень (из отриц. делает полож)


//var5
let [a, b, c, d] = prompt().padStart(4,0);
console.log( [11, 1][Number((a + b) == (d + c) )] );


